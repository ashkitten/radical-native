{
  "description": "Extending Riot Web with native messaging capabilities",
  "manifest_version": 2,
  "name": "Radical Native",
  "version": "0.1beta3",
  "applications": {
    "gecko": {
      "id": "@radical-native",
      "strict_min_version": "68.0"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["contextualIdentities", "management", "nativeMessaging"]
}
